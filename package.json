{
  "name": "@femboycafe/create",
  "displayName": "Create TS",
  "version": "0.1.3",
  "description": "A templating tool for a variety of different typescript utilities",
  "main": "dist/index.js",
  "scripts": {
    "build": "rollup -c --bundleConfigAsCjs",
    "dev": "echo $ build && rollup -c --bundleConfigAsCjs && echo $ start && node .",
    "start": "node .",
    "prepublish": "node prepublish.js && pnpm build",
    "postupgrade": "pnpm i chalk@^4"
  },
  "keywords": [
    "typescript",
    "create-app",
    "create-typescript",
    "typescript-electron",
    "electron-sveltekit",
    "svelte",
    "sveltekit",
    "webdev",
    "web",
    "electron",
    "template",
    "templating",
    "scaffolding"
  ],
  "author": "Exponential-Workload",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.0",
    "@types/command-exists": "^1.2.0",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^18.15.11",
    "@types/prompts": "^2.4.4",
    "rollup": "^3.20.2",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@exponentialworkload/logger": "^1.0.10",
    "chalk": "^4.1.2",
    "command-exists": "^1.2.9",
    "fs-extra": "^11.1.1",
    "get-cursor-position": "^2.0.0",
    "node-gyp": "^9.3.1",
    "prompts": "^2.4.2"
  },
  "homepage": "https://github.com/Exponential-Workload/create-ts",
  "bin": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Exponential-Workload/create-ts.git"
  }
}